{
  "name": "beewebapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "install:dependencies": "npm install && cd front_app && npm install && cd ../",
    "app:start": "cd front_app && npm run build && cd ../ && npm start"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@hapi/joi": "^17.1.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.18.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "sequelize": "^5.21.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "sequelize-cli": "^5.5.1"
  },
  "nodemonConfig": {
    "ignore": [
      "front_app/*"
    ]
  }
}
